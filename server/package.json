{
  "name": "roborecipes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "server": "ts-node ./src/app",
    "local": "nodemon ./src/app.ts",
    "develop": "concurrently --kill-others-on-fail \"npm run server\" \" cd ../ && npm run dev --prefix client\"",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"cd ../ && npm run dev --prefix client\""
  },
  "engines": {
    "node": "18.6.0"
  },
  "dependencies": {
    "aws-sdk": "^2.1293.0",
    "axios": "^1.2.2",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "concurrently": "^7.3.0",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "express-flash": "^0.0.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.0",
    "http-errors": "~1.6.3",
    "image-downloader": "^4.3.0",
    "jade": "~1.11.0",
    "luxon": "^3.0.3",
    "mongoose": "^6.5.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.19",
    "openai": "^3.1.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "~1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "~4.1.7",
    "@types/express": "~4.16.1",
    "@types/express-flash": "^0.0.2",
    "@types/express-formidable": "^1.2.0",
    "@types/express-session": "^1.17.5",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "~1.6.3",
    "@types/jade": "~0.0.31",
    "@types/luxon": "^3.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "~1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/nodemon": "^1.19.2",
    "@types/passport": "^1.0.11",
    "@types/passport-local": "^1.0.34",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
